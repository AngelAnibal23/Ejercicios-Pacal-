program OrdenDecreciente;

var
    num1, num2, num3: Integer;
begin

    writeln('Ingrese el primer número: ');
    readln(num1);
    writeln('Ingrese el segundo número: ');
    readln(num2);
    writeln('Ingrese el tercer número: ');
    readln(num3);

    if (num1 > num2) and (num2 > num3) then
        writeln('Los números están en orden decreciente.')
    else
        writeln('Los números NO están en orden decreciente.');

    readln;
end.
